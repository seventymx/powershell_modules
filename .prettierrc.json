{
    "singleQuote": false,
    "printWidth": 200,
    "proseWrap": "preserve",
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "semi": true,
    "endOfLine": "lf",
    "overrides": [
        {
          "files": "*.md",
          "options": {
            "proseWrap": "always"
          }
        },
        {
          "files": "*.yml",
          "options": {
            "tabWidth": 2
          }
        }
      ]
}
